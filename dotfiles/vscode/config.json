{
    "diffEditor.renderSideBySide": false,
    "editor.minimap.enabled": false,
    "editor.fontSize": 14,
    "editor.fontFamily": "'JetBrains Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.renderWhitespace": "none",
    "editor.bracketPairColorization.enabled": true,
    "explorer.confirmDragAndDrop": false,
    "files.exclude": {
        "**/__pycache__": true,
        "**/__pypackages__": true,
        "**/.git": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/.venv": true,
        "**/.vscode": true,
        "**/build": true,
        "**/venv": true,
    },
    "files.associations": {
        "*.env": "shellscript",
        "*.env.*": "shellscript",
        "*.*ignore": "ignore"
    },
    "files.trimTrailingWhitespace": true,
    "files.insertFinalNewline": true,
    "files.watcherExclude": {
        "**/__pycache__": true,
        "**/.git": true,
        "**/.mypy_cache": true,
        "**/.pytest_cache": true,
        "**/.venv": true,
        "**/.vscode": true,
        "**/venv": true
    },
    "git.openRepositoryInParentFolders": "always",
    "markdown-preview-enhanced.previewTheme": "monokai.css",
    "markdown-preview-enhanced.mermaidTheme": "dark",
    "redhat.telemetry.enabled": false,
    "security.workspace.trust.untrustedFiles": "open",
    "telemetry.telemetryLevel": "off",
    "window.title": "${rootName}${separator}${dirty}${activeEditorShort}",
    "workbench.colorTheme": "Monokai",
    "workbench.enableExperiments": false,
    "workbench.settings.enableNaturalLanguageSearch": false,
    "workbench.startupEditor": "none",
    "extensions.ignoreRecommendations": true,
    "[python]": {
        "editor.formatOnSave": true,
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit",
        }
    },
    "ruff.lint.args": [
        "--config=pyproject.toml"
    ],
    "ruff.organizeImports": true,
    "ruff.fixAll": true,
    "editor.formatOnSave": true,
    "[rust]": {
        "editor.formatOnSave": true
    },
    "python.autoComplete.extraPaths": [
        "${workspaceFolder}/__pypackages__/3.10/lib",
        "${workspaceFolder}/__pypackages__/3.11/lib"
    ],
    "update.showReleaseNotes": false,
    "workbench.editor.empty.hint": "hidden",
    "workbench.activityBar.location": "hidden",
    "python.analysis.extraPaths": [
        "${workspaceFolder}/__pypackages__/3.10/lib",
        "${workspaceFolder}/__pypackages__/3.11/lib"
    ],
    "window.zoomLevel": 2,
    "rust-analyzer.inlayHints.parameterHints.enable": false,
    "ruff.lint.run": "onSave",
    "python.analysis.inlayHints.variableTypes": true,
    "workbench.iconTheme": "material-icon-theme",
    "rust-analyzer.check.extraEnv": {},
    "workbench.editorAssociations": {
        "*.csv": "default"
    },
}
